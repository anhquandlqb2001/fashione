<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="address"
            type="vn.quanprolazer.fashione.domain.models.BaseAddressPickupImpl" />
    </data>

    <TextView
        android:id="@+id/tv_address_pickup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:text="@{address.nameWithType}"
        tools:text="Quang Binh" />
</layout>
